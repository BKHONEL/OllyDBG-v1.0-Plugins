/*这是一个SkyPatch补丁脚本样例功能是让一批按钮失效*/@0x00405E64:	jmp @start ;从此跳到补丁代码的开始@$backinit:  	;语句结束标号，标识从补丁代码跳回的地址@0x00402630:　;串标号，标识串块开始$str1 "this is the first string"         ;串标号		"jjjs this is the first string"$str2 #45efe3003455667898df225600990000#@$: ;语句结束标号，标识串块结束@start= @$ + 0x4:	;两种形式的eip标号，串块结束标号来指定的（串块结束标号[加或减]一个十六进制的常数）;@0x00401000:;直接指定地址的eip标号	call 406210 ;;;从init搬到此运行，原来的改为跳到此 jmp 00463EB6	push 0 ;此用于init时，	call 463dd0	add esp,4	test  eax,eax	jne @back   ;是0，禁止  push esi  mov esi,$str2 ;0x463fe0@loop3:    xor ecx,ecx  mov cx, word ptr [esi]  test ecx,ecx  je @exitloop3 ;跳出loop  push 0  push ecx  mov ecx,dword ptr ss:[ebp-4]  call 0045BBB2  mov ecx,eax  call 0045BBE2  inc esi  inc esi  jmp @loop3  @exitloop3:    pop esi@back:  	jmp @$backinit ; 405E64 ;跳回原代码  